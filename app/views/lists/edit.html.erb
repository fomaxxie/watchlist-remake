<h1>Update <%=@list.name%></h1>

<%= form_with model: @list do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, placeholder: "Erotica" %>
  <%= f.submit %>
<% end %>


<p><%= link_to "Delete", list_path(@list),  data: {turbo_method: :delete, turbo_confirm: "You sure?"} %><p>

<p><%= link_to "Back", root_path %></p>
